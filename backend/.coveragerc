[run]
branch = True
source = .

[report]
fail_under = 98
ignore_errors = True
show_missing = True
skip_covered = True
omit =
    api/v1/admin/management/*
    core/*
    common/health_checks.py
    common/rest_framework/permissions.py
    common/social_core/backends.py
    common/social_core/pipeline.py
    manage.py
    tests/*
    */admin.py
    */tests.py
exclude_lines =
    pragma: no cover
    if settings.DEBUG
    if getattr.*"swagger_fake_view".*
    return .*[.]none[(][)]