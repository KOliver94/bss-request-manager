{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@mui/icons-material": "5.15.5",
    "@mui/material": "5.15.5",
    "@mui/x-date-pickers": "6.19.0",
    "@sentry/react": "7.94.1",
    "axios": "1.6.5",
    "classnames": "2.5.1",
    "date-fns": "3.3.0",
    "jwt-decode": "4.0.0",
    "mui-tel-input": "5.0.0",
    "notistack": "3.0.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "3.1.0",
    "react-hook-form": "7.49.3",
    "react-router-dom": "6.21.3",
    "sass": "1.70.0",
    "yup": "1.3.3"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.jsx src/",
    "check-eslint-config": "eslint-config-prettier src/index.js",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vite-pwa/assets-generator": "0.2.3",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "3.2.4",
    "source-map-explorer": "2.5.3",
    "vite": "5.0.12",
    "vite-plugin-pwa": "0.17.4"
  }
}
