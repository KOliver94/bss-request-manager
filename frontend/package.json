{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@hookform/resolvers": "5.2.2",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "@mui/x-date-pickers": "8.23.0",
    "@sentry/react": "10.32.1",
    "axios": "1.13.2",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "jwt-decode": "4.0.0",
    "mui-tel-input": "9.0.1",
    "notistack": "3.0.2",
    "prop-types": "15.8.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-google-recaptcha": "3.1.0",
    "react-hook-form": "7.69.0",
    "react-router": "7.11.0",
    "sass": "1.97.1",
    "yup": "1.7.1"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,json,css,scss}",
    "lint": "eslint --ext .js,.jsx src/",
    "lint:fix": "eslint --fix --ext .js,.jsx src/",
    "check-eslint-config": "eslint-config-prettier src/index.js",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@typescript-eslint/parser": "8.51.0",
    "@vite-pwa/assets-generator": "1.0.2",
    "@vitejs/plugin-basic-ssl": "2.1.0",
    "@vitejs/plugin-react": "5.1.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "source-map-explorer": "2.5.3",
    "typescript-eslint": "8.51.0",
    "vite": "7.3.0",
    "vite-plugin-pwa": "1.2.0",
    "vite-tsconfig-paths": "6.0.3"
  }
}
