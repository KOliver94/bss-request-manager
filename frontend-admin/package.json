{
  "name": "frontend-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "9.1.0",
    "@tanstack/react-query": "5.66.0",
    "axios": "1.7.9",
    "bootstrap-icons": "1.11.3",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primelocale": "primefaces/primelocale",
    "primereact": "10.9.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-router": "7.1.5",
    "timeago.js": "4.0.2"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend-admin/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx",
    "generate-client": "openapi-generator-cli generate && prettier --write src/api/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/js": "9.20.0",
    "@openapitools/openapi-generator-cli": "2.16.3",
    "@tanstack/eslint-plugin-query": "5.66.1",
    "@tanstack/react-query-devtools": "5.66.0",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "15.15.0",
    "postcss": "8.5.2",
    "prettier": "3.5.0",
    "source-map-explorer": "2.5.3",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.0",
    "vite": "6.1.0",
    "vite-tsconfig-paths": "5.1.4"
  }
}
