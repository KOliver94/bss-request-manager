{
  "name": "frontend-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@sentry/react": "7.31.1",
    "bootstrap-icons": "1.10.3",
    "primeflex": "3.3.0",
    "primeicons": "6.0.1",
    "primereact": "8.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.1",
    "react-router-dom": "6.7.0",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx"
