{
  "name": "frontend-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "9.10.1",
    "@tanstack/react-query": "5.71.0",
    "axios": "1.8.4",
    "bootstrap-icons": "1.11.3",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primelocale": "primefaces/primelocale",
    "primereact": "10.9.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.55.0",
    "react-router": "7.4.1",
    "timeago.js": "4.0.2"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend-admin/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx",
    "generate-client": "openapi-generator-cli generate && prettier --write src/api/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@openapitools/openapi-generator-cli": "2.18.4",
    "@tanstack/eslint-plugin-query": "5.68.0",
    "@tanstack/react-query-devtools": "5.71.0",
    "@types/node": "22.13.14",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.21",
    "cssnano": "7.0.6",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.3.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.0.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "source-map-explorer": "2.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0",
    "vite": "6.2.3",
    "vite-tsconfig-paths": "5.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.38.0",
    "@rollup/rollup-linux-x64-musl": "4.38.0",
    "@rollup/rollup-win32-x64-msvc": "4.38.0"
  }
}
