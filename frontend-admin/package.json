{
  "name": "frontend-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "10.15.0",
    "@tanstack/react-query": "5.90.2",
    "axios": "1.12.2",
    "bootstrap-icons": "1.13.1",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primelocale": "primefaces/primelocale",
    "primereact": "10.9.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.63.0",
    "react-router": "7.9.3",
    "timeago.js": "4.0.2"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend-admin/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx",
    "generate-client": "openapi-generator-cli generate && prettier --write src/api/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@openapitools/openapi-generator-cli": "2.24.0",
    "@tanstack/eslint-plugin-query": "5.91.0",
    "@tanstack/react-query-devtools": "5.90.2",
    "@types/node": "22.18.6",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "autoprefixer": "10.4.21",
    "cssnano": "7.1.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.4.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "source-map-explorer": "2.5.3",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vite-tsconfig-paths": "5.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.3",
    "@rollup/rollup-linux-x64-musl": "4.52.3",
    "@rollup/rollup-win32-x64-msvc": "4.52.3"
  }
}
