{
  "name": "frontend-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "7.108.0",
    "@tanstack/react-query": "5.28.6",
    "axios": "1.6.8",
    "bootstrap-icons": "1.11.3",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primelocale": "primefaces/primelocale",
    "primereact": "10.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.1",
    "react-router-dom": "6.22.3",
    "timeago-react": "3.0.6"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend-admin/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx",
    "generate-client": "openapi-generator-cli generate && prettier --write src/api/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.13.1",
    "@tanstack/eslint-plugin-query": "5.28.6",
    "@tanstack/react-query-devtools": "5.28.6",
    "@types/node": "20.11.30",
    "@types/react": "18.2.69",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "cssnano": "6.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "source-map-explorer": "2.5.3",
    "typescript": "5.4.3",
    "vite": "5.2.3",
    "vite-tsconfig-paths": "4.3.2"
  }
}
