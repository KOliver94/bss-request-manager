{
  "name": "frontend-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@sentry/react": "8.53.0",
    "@tanstack/react-query": "5.66.0",
    "axios": "1.7.9",
    "bootstrap-icons": "1.11.3",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primelocale": "primefaces/primelocale",
    "primereact": "10.9.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.54.2",
    "react-router": "7.1.5",
    "timeago-react": "3.0.6"
  },
  "scripts": {
    "start": "vite dev",
    "start:network": "vite dev --host",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/frontend-admin/*.js' --no-border-checks",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css}",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "check-eslint-config": "eslint-config-prettier src/index.tsx",
    "generate-client": "openapi-generator-cli generate && prettier --write src/api/**/*.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.16.3",
    "@tanstack/eslint-plugin-query": "5.66.0",
    "@tanstack/react-query-devtools": "5.66.0",
    "@types/node": "22.13.0",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "4.6.2",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "source-map-explorer": "2.5.3",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vite-tsconfig-paths": "5.1.4"
  }
}
